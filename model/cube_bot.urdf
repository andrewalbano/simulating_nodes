<?xml version="1.0"?>
<robot name="cube_bot">

  <!-- Link: base_link -->
  <link name="base_link">
    <visual>
      <geometry>
        <box size= "0.5 0.5 0.5"> <!--"1 1 1" Dimensions of the cube -->
      </geometry>
      <material name="blue">
        <color rgba="0.0 0.0 1.0 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="0.5 0.5 0.5"> <!--"1 1 1" -->
      </geometry>
    </collision>
    <inertial>
      <mass value="1.0"/>
      <origin xyz="0 0 0"/>
      <!-- Simple inertia tensor for a uniform cube -->
      <inertia ixx="0.166" ixy="0.0" ixz="0.0" iyy="0.166" iyz="0.0" izz="0.166"/>
    </inertial>
  </link>

</robot>
